<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mudaliyar.shekar.mldcc_clone">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ResultActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".finalResult"
            android:parentActivityName=".ResultActivity" />
        <activity
            android:name=".NoticePdfActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EventsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".GalleryActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LibraryActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NotificationActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NavPrinMsg"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ActivitiesActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CoursesActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StaffActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ConvocationActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FeedbackActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ImageViewActivity"/>

        <service
            android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

        <service
            android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>